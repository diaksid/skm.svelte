{
  "name": "skm.svelte",
  "version": "1.0.1",
  "description": "construction company «masshtab»",
  "repository": {
    "type": "git",
    "url": "https://github.com/stroyexp/skm.svelte.git"
  },
  "author": "DVA",
  "homepage": "https://skm.moscow",
  "private": true,
  "scripts": {
    "clean": "rimraf .svelte-kit && rimraf build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "start": "node build/index.js",
    "cluster": "node run/cluster.js",
    "dev": "vite dev --port=3000",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "generate:sitemap": "node run/generate.sitemap.js",
    "generate:images": "node run/generate.image-data.js",
    "generate:css": "npx tailwindcss -i ./static/assets/stylesheets/search.css -o ./static/assets/stylesheets/search.min.css --minify --watch",
    "build": "vite build",
    "preview": "vite preview --port=3000",
    "test": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@iconify/svelte": "*",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/aspect-ratio": "*",
    "@tailwindcss/typography": "*",
    "@types/sharp": "*",
    "autoprefixer": "*",
    "cssnano": "*",
    "cssnano-preset-advanced": "*",
    "postcss": "*",
    "postcss-import": "*",
    "postcss-load-config": "*",
    "sharp": "*",
    "svelte": "*",
    "svelte-check": "*",
    "svelte-preprocess": "*",
    "tailwindcss": "*",
    "tslib": "*",
    "typescript": "*",
    "vanilla-lazyload": "*",
    "vite": "*",
    "vite-imagetools": "*"
  },
  "type": "module",
  "dependencies": {
    "@playwright/test": "*",
    "@types/html-minifier": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "dotenv": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-svelte3": "*",
    "html-minifier": "*",
    "prettier": "*",
    "prettier-plugin-svelte": "*",
    "cross-env": "*",
    "gzipper": "*",
    "rimraf": "*",
    "vitest": "*"
  }
}
